  }

  //Nombre UpperCase Regex Restriction
  var regexname =/\w*[A-Z]\w*[A-Z]\w*/;
  var texto = ' '+$('.views-field-field-nombre .text-full').val();
  $('.views-field-field-nombre .text-full').on('keypress keydown keyup',function()
  {
             if (!$(this).val().match(regexname)) 

             {
              
              texto = $('.views-field-field-nombre .text-full').val();
                
             }
           else{
                  
                texto = texto.toLowerCase().replace(/(^.)|(\s[a-z])/g, function(letter)
                 {
                  return letter.toUpperCase();
                 });
                
                $('.views-field-field-nombre .text-full').val(texto);
    
               }
         });

   $('.views-field-field-nombre .text-full').blur(function() {

      texto = texto.toLowerCase().replace(/(^.)|(\s[a-z])/g, function(letter)
                 {
                  return letter.toUpperCase();
                 });
               
      $('.views-field-field-nombre .text-full').val(texto);

    });



    //AP UpperCase Regex Restriction
 
  var textoP = $('.views-field-field-apellido-paterno .text-full').val();
  $('.views-field-field-apellido-paterno .text-full').on('keypress keydown keyup',function()
  {
             if (!$(this).val().match(regexname)) 

             {
              
              textoP = $('.views-field-field-apellido-paterno .text-full').val();
                
             }
