    $('#edit-field-participaciones-especiales-1-0-field-participaciones-especiales-und-'+i+'-target-id').attr('readonly', true);
    $('#edit-field-participaciones-especiales-1-0-field-participaciones-especiales-und-'+i+'-target-id--'+j).attr('readonly', true);
